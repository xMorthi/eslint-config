  "exports": {
    ".": {
      "import": "./dist/flat.mjs",
      "require": "./dist/flat.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@nuxt/eslint-config": "^0.6.0",
    "eslint": "^9.12.0"
  },
  "devDependencies": {
    "jiti": "^2.3.3",
    "unbuild": "^2.0.0"
  }
}
